# Rewrite Engine
RewriteEngine on

# Set index
DirectoryIndex framework.php

# Rewrite Cond
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Get and set page in variable
RewriteRule ^(.*) framework.php?route=$1 [L]

